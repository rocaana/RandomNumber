
    r= zeros(1, 10000);

    for i = 1:10000
        
        r1 = randi(5);
        r2 = randi(5);
        r3 = randi(5);
        r4 = randi(5);
        r5 = randi(5);

        r(i) = mod((r1+r2+r3+r4+r5)*5,7) +1 ;
        
    end
      
    histogram(r);
    xlabel('Valor');
    ylabel('Frecuencia');
